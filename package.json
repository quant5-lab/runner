{
  "name": "pinets-example-app",
  "version": "1.0.0",
  "description": "Lightweight Node.js app demonstrating PineTS usage",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"node --watch index.js\" \"http-server -p 8080 -c-1\"",
    "run": "node index.js",
    "stop": "pkill -f http-server || true"
  },
  "dependencies": {
    "pinets": "file:../PineTS"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "http-server": "^14.1.1"
  }
}
