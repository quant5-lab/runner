//@version=5
indicator(title="20-50-200 SMA", shorttitle="SMA Lines", overlay=true)

ma20 = ta.sma(close, 20)
ma50 = ta.sma(close, 50)
ma200 = ta.sma(close, 200)

plot(ma20, color=color.yellow, title="SMA20", linewidth=2)
plot(ma50, color=color.green, title="SMA50", linewidth=2)
plot(ma200, color=color.red, title="SMA200", linewidth=2)
